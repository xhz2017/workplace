package com.luoExpress.interfaceDemo;

public class Test_Laptop {
	public static void main(String[] args) {
		Laptop la = new Laptop();
		Mouse mouse = new Mouse();
		Keyboard keyboard = new Keyboard();
		la.run();
		la.OpenUsb(mouse);
		la.OpenUsb(keyboard);
		la.closeUsb(mouse);
		la.closeUsb(keyboard);
		la.closedown();
	}
}
