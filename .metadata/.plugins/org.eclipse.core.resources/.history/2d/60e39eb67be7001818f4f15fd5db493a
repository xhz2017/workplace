package com.heima.threadmethod;

public class Allan1_Name {
	public static void main(String[] args) {
		new Thread(){
			public void run(){
				System.out.println(this.getName()+"aaaa");
			}
		}.start();
		new Thread(){
			public void run(){
				System.out.println(this.getName()+"bbbb");
			}
		}.start();
	}
}
