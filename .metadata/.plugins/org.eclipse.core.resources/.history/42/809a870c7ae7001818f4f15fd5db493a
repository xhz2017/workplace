			new Allan_Demo();
		}
	}

	private static void demo2() {
		TestThread al = new TestThread();
		al.start();
		for(int i = 0; i < 100; i++) {
			System.out.println("aaaaaaaaaaaaaaaaa");
		}
	}
}

