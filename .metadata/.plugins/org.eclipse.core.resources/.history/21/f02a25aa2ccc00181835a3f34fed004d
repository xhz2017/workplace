public class Se07_Exercise08 {
	public static void main(String[] args) {
		ArrayList arraylist = new ArrayList();

		
		Employee emp1 = new Employee();
		emp1.name = "张山";
		emp1.salary = 1800;
		emp1.cardNo = "123";
		arraylist.add(emp1);
		Employee emp2 = new Employee();
		emp2.name = "李四";
		emp2.salary = 2800;
		emp2.cardNo = "456";
		arraylist.add(emp2);
		Employee emp3 = new Employee();
		emp3.name = "王五";
		emp3.salary = 3800;
		emp3.cardNo = "789";
		arraylist.add(emp3);
	}
}
