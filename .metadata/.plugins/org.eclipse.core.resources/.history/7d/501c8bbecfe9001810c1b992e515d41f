package com.allan.factory;

public class AnimalFactory {
/*	public static Dog createdog(){
		return new Dog();
	}
	public static Cat createcat(){
		return new Cat();
	}*/
	public static Animal createAnimal(String name){
		if("dog".equals(name)){
			return new Dog();
		}else if("cat".equals(name)){
			return new Cat();
		}else{
			return null
		}
	}
}
