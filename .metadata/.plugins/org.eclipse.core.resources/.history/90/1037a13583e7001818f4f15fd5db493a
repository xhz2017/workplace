package com.heima.threadmethod;

public class Allan5_Join {
	public static void main(String[] args) {
		Thread t1 = new Thread(){
			public void fun(){
				for(int i=0; i<10;i++){
					System.out.println(getName()+"...aaa");
				}
			}
		};
		Thread t2 = new Thread(){
			public void fun(){
				for(int i=0; i<10;i++){
					System.out.println(getName()+"...aaa");
				}
			}
		};
	}
}
