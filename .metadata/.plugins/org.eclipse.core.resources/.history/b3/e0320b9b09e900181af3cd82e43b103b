package com.allan.thread;



public class Demo1_Notify {
	public static void main(String[] args) {
		
	}
}
class Allan_Print{
	public void print1(){
		synchronized (this) {
			System.out.print("I");
			System.out.print("love");
			System.out.print("you");
			System.out.println();
		}
	}
	public void print2(){
		synchronized (this) {
			System.out.print("wu");
			System.out.print("xiao");
			System.out.print("hong");
			System.out.println();
		}
	}
}