package com.luoExpress.Demo;

public class Demo_String {
	public static void main(String[] args) {
		String string = new String("sfasafd");

		find(string);

	}

	private static void find(String string) {
		int daxie = 0;
		int xiaoxie = 0;
		int shuzi = 0;
		int other=0;

		char[] arr = string.toCharArray();
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] >= 'A' && arr[i] <= 'Z') {
				daxie++;
			}else if(arr[i] >= 'a' && arr[i] <= 'z'){
				xiaoxie++;
			}if(arr[i] >= '0' && arr[i] <= '9'){
				shuzi++;
			}else{
				other++;
			}
		}
	}
}
