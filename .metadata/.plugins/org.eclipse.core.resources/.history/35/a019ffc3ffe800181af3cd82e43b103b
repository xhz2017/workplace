package com.heima.thread;

public class Allan1_Singleton {
	public static void main(String[] args) {
		Allan_SingleTon st1 = Allan_SingleTon.getInstance();
		Allan_SingleTon st2 = Allan_SingleTon.getInstance();
		System.out.println(st1==st2);
	}
}


class Allan_SingleTon {

	private Allan_SingleTon() {
		super();
	}

	private static Allan_SingleTon s;

	public static Allan_SingleTon getInstance() {
		if(s==null){
			 s= new Allan_SingleTon()
		}
		return s;
	}
}

