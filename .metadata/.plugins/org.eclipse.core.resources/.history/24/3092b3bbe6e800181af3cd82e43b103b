package com.heima.syn;

public class Allan1_Synchronized {
	public static void main(String[] args) {
		final Allan_Print ap = new Allan_Print();
		new Thread(){
			public void run(){
				for (int i = 0; i < 50; i++) {
					ap.print1();	
				}
			}
		}.start();
		new Thread(){
			public void run(){
				for (int i = 0; i < 50; i++) {
					ap.print2();	
				}
			}
		}.start();
	}
}

class Allan_Print{
	Demo demo= new Demo();
	public void print1(){
		System.out.print("I");
		System.out.print("love");
		System.out.print("you");
		System.out.println();
	}
	public void print2(){
		System.out.print("wu");
		System.out.print("xiao");
		System.out.print("hong");
		System.out.println();
	}
}

