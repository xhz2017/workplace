package com.luo.exception;

public class Test {

	public static void main(String[] args) {
		int[] arr = { 1, 2, 4, 6, 85, 3, 68, 90, 7 };
		// sortArray(arr);
		// sortArray2(arr);
		//insertArray(arr);
		//printArray(arr);
		//Code_12_SmallSum.s
	}

	private static void printArray(int[] arr) {
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i] + " ");
		}
	}

	private static void sortArray(int[] arr) {
		if (arr == null)
			return;
		if (arr.length < 2)
			return;
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr.length - i - 1; j++) {
				if (arr[j] > arr[j + 1]) {
					int temp = arr[j + 1];
					arr[j + 1] = arr[j];
					arr[j] = temp;
				}
			}
		}

	}

	private static void sortArray2(int[] arr) {
		if (arr == null)
			return;
		if (arr.length < 2)
			return;
		for (int i = 0; i < arr.length; i++) {
			for (int j = i + 1; j < arr.length; j++) {
				if (arr[i] > arr[j]) {
					int temp = arr[j];
					arr[j] = arr[i];
					arr[i] = temp;
				}
			}
		}
	}

	private static void insertArray(int[] arr) {
		if (arr == null)
			return;
		if (arr.length < 2)
			return;
		for (int i = 1; i < arr.length; i++) {
			for (int j = i; j > 0; j--) {
				if (arr[j - 1] > arr[j]) {
					int temp = arr[j];
					arr[j] = arr[j - 1];
					arr[j - 1] = temp;
				} else {
					break;
				}
			}
		}
		// printArray(arr);
	}
}
