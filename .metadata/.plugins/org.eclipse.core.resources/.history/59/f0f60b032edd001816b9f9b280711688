package com.luoExpress.Eclipse;

public class Student {
	private String name;
	private float yuwenGrade;
	private float shuxuGrade;
	private float yingyuGrade;
	private float totalGrade;
	public Student() {
		super();
	}
	public Student(String name, float yuwenGrade, float shuxuGrade,
			float yingyuGrade) {
		super();
		this.name = name;
		this.yuwenGrade = yuwenGrade;
		this.shuxuGrade = shuxuGrade;
		this.yingyuGrade = yingyuGrade;
		this.totalGrade = yuwenGrade+shuxuGrade+yingyuGrade;
	}

}
