	/*
	 * 获取幸运数字
	 * 1,返回值类型int
	 * 2,参数列表int num
	 */
	public static int getLuck(int num){
		ArrayList<Integer> alist = new ArrayList<>();
		for (int i = 0; i < num; i++) {
			alist.add(i);
		}
		
		while(alist.size()>1){
			
		}
		return 0;
	}
}
