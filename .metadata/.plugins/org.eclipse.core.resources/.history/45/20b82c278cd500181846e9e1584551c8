package com.luoExpress.interfaceDemo;

public class Laptop{
	Mouse mouse=new Mouse();
	Keyboard keyboard=new Keyboard();
	public void run(){
		
		mouse.open();
		keyboard.open();
		System.out.println("runing");
	}
	public void Openusb(USB usb){
		
		usb.open();
		System.out.println("usb open");
	}
	public void Closeusb(USB usb){
		
		usb.open();
		System.out.println("usb open");
	}
	public void closedown(){
		mouse.close();
		keyboard.close();
		System.out.println("closedown");
	}
}
