package com.luo.map;

import java.util.HashMap;
import java.util.Map;

public class DemoMap {
	public static void main(String[] args) {
		Map<String, String> map = new HashMap<String, String>();
		map.put("1", "Mon");
		map.put("2", "Tues");
		 System.out.println(map);
		 System.out.println(map.put("1", "mo"));
	}
}
