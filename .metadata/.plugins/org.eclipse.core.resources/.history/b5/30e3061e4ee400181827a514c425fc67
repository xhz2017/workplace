
public class Demo_Regex {
	public static void main(String[] args) {
		//demo1();
		//demo2();
		String s = "18-22-40-65";
		String regex = "-";
		String[] str=s.split(regex);
	}

	private static void demo2() {
		//∆•≈‰qq∫≈¬Î
		String qq="87032585";
