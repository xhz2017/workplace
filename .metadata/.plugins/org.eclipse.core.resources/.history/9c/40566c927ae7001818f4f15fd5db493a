package com.heima.thread;

public class Allan_Thread {
	public static void main(String[] args) {
		demo2();
		//demo1();
	}

	private static void demo1() {
		for (int i = 0; i < 10000000; i++) {
			new Allan_Demo();
		}
	}

	private static void demo2() {
		TestThread al = new TestThread();
		al.start();
		for(int i = 0; i < 10; i++) {
			System.out.println("aaaaaaaaaaaaaaaaa");
		}
	}
}

class Allan_Demo{

	@Override
	public void finalize(){
		System.out.println(Thread.currentThread().getName()+" À¬»ø±»ÇåÉ¨ÁË");
	}
}

class TestThread extends Thread{
	public void run(){
		for (int i = 0; i < 10; i++) {
			System.out.println(this.getName()+"allan thread");	
		}
	}
}