package com.leetcode.exercise;

public class LeetCode_CountSegments {
	public static void main(String[] args) {
		String s=", , , ,        a, eaefa";
		System.out.println(countSegments(s));
	}
    public static int countSegments(String s) {
    	if("".equals(s)){
    		return 0;
    	}
    	String[] array= s.split("");
        return array.length;
    }
}
