package com.heima.syn;

public class Allan5_DeadLock {

	public static void main(String[] args) {
		final String s1 ="×ó¿ê×Ó";
		final String s2 ="ÓÒ¿ê×Ó";
		new Thread(){
			public void run(){
				for(int i=0;i<10;i++){
					synchronized (s1) {
						System.out.println("µÈ´ý"+s2);
						synchronized (s2) {
							System.out.println("³Ô·¹");
						}
					}			
				}

			}
		}.start();
		
		new Thread(){
			public void run(){
				for (int i = 0; i < 10; i++) {
					synchronized (s2) {
						System.out.println("µÈ´ý"+s1);
						synchronized (s1) {
							System.out.println("³Ô·¹");
						}
					}					
				}
			}
		}.start();
	}

}
