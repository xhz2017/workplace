package com.heima.exception;

public class Student {

	private String name;
	private int grade;
	public Student() {
		super();
	}
	public Student(String name, int grade) {
		super();
		this.name = name;
		this.grade = grade;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getGrade() {
		return grade;
	}
	public void setGrade(int grade) {
		if (grade<0){
			throw new GradeInvalide("³É¼¨ÎÞÐ§");
		}
		this.grade = grade;
	}

	
}
