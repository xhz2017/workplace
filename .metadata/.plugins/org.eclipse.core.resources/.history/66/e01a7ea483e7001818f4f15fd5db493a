package com.heima.threadmethod;

public class Allan5_Join {
	public static void main(String[] args) {
		 Thread t1 = new Thread(){
			public void run(){
				for(int i=0; i<10;i++){
					t2.join();
					System.out.println(getName()+"...aaa");
				}
			}
		};
		final Thread t2 = new Thread(){
			public void run(){
				for(int i=0; i<10;i++){
					System.out.println(getName()+"..bb");
				}
			}
		};
		t1.start();
		t2.start();
	}
}
